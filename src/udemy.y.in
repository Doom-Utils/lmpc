%{
  #ifdef HAVE_CONFIG_H
    #include <config.h>
  #endif
  #include <stdlib.h> /* malloc, free */
  #include <string.h> /* strcat */
  #ifdef HAVE_MEMORY_H
    #include <memory.h>
  #endif
  #include "tools.h"  /* error */
  #include "node.h"   /* nodes */

  #define YYSTYPE nodep

  #include "record.h" /* SERVERVERSION */
  #include "binblock.h"
  #include "udem.h"
  #include "uqwd.h"
  #include "udm2.h"

  typedef struct {
    int type;
    int obli;
  } CO_t;

  void reorder_commands(node* m, CO_t* co, int count);

  extern void yyerror(char* s);
  extern int yylex(void);
%}

		/* values */
%token v_int
%token v_float
%token v_string

		/* token */
@YACCTOKEN@
%%

input:			dem_input | qwd_input | dm2_input ;

dem_input:		trackblock dem_datablocks ;

qwd_input:		qwd_datablocks ;

dm2_input:		dm2_datablocks | dm2_datablocks dm2_endblock ;

trackblock: TOKEN_TRACK v_string ';' {
			node_add_down($1,$2);
			do_block($1);
			node_delete($1);
		};

dem_datablocks:	dem_datablock | dem_datablocks dem_datablock;

qwd_datablocks: qwd_datablock | qwd_datablocks qwd_datablock;

dm2_datablocks:	dm2_datablock | dm2_datablocks dm2_datablock;

dem_datablock: TOKEN_BLOCK '{' dem_messages '}' {
			node_add_down($1,$3);
			do_block($1);
			node_delete($1);
		};

qwd_datablock: qwd_sblock | qwd_cblock | qwd_fblock ;

qwd_sblock: qwd_sgameblock | qwd_sconnlessblock ;

qwd_sgameblock: TOKEN_SBLOCK '{' qwd_t12r qwd_sgamemessages '}' {
			node_add_next($3,$4);
			node_add_down($1,$3);
			do_block($1);
			node_delete($1);
		}
		| TOKEN_SBLOCK '{' qwd_t12r '}' {
			node_add_down($1,$3);
			do_block($1);
			node_delete($1);
		}

qwd_sconnlessblock: TOKEN_SBLOCK '{' qwd_sconnlessmessages '}' {
			node_add_down($1,$3);
			do_block($1);
			node_delete($1);
		};

qwd_cblock: TOKEN_CBLOCK '{' qwd_cmessages '}' {
			CO_t cblock_cs[] = {
			  { TOKEN_TIME,      1 },
			  { TOKEN_LOAD,      1 },
			  { TOKEN_ANGLES,    1 },
			  { TOKEN_SPEED,     1 },
			  { TOKEN_FIRE,      0 },
			  { TOKEN_JUMP,      0 },
			  { TOKEN_FLAG,      0 },
			  { TOKEN_IMPULSE,   0 },
			  { TOKEN_UK_ANGLES, 1 }
			};
			node_add_down($1,$3);
			reorder_commands($1, cblock_cs, arraylength(cblock_cs));
			do_block($1);
			node_delete($1);
		};

qwd_fblock: TOKEN_FBLOCK '{' qwd_t12r '}' {
			node_add_down($1,$3);
			do_block($1);
			node_delete($1);
		};

dm2_datablock: dm2_realdatablock | dm2_betweenblock;

dm2_realdatablock: TOKEN_BLOCK '{' dm2_messages '}' {
			node_add_down($1,$3);
			do_block($1);
			node_delete($1);
		};

dm2_betweenblock: TOKEN_BETWEENBLOCK ';' {
			do_block($1);
			node_delete($1);
		};

dm2_endblock: TOKEN_ENDBLOCK ';' {
			do_block($1);
			node_delete($1);
		};
		
dem_messages: m_camera {
			$$=$1;
		}
		| m_camera dem_o_messages {
			$$=node_add_next($1,$2);
		}
		| dem_o_messages m_camera {
			$$=node_add_next($2,$1);
		}
		| dem_o_messages m_camera dem_o_messages {
			$$=node_add_next($2,$1);
			node_add_next($$,$3);
		}

dem_o_messages: dem_message {
			$$=$1;
		}
		| dem_o_messages dem_message {
			$$=node_add_next($1,$2);
		};

qwd_sgamemessages: qwd_sgamemessage { $$=$1; }
		| qwd_sgamemessages qwd_sgamemessage
			{ $$=node_add_next($1,$2); };

qwd_sconnlessmessages: m_time qwd_connless { $$=node_add_next($1,$2); }
		| qwd_connless m_time { $$=node_add_next($2,$1); } ;

qwd_connless: TOKEN_CONNLESS '{' qwd_sconnlessmessage '}' {
			$$=node_add_down($1,$3); } ;

qwd_cmessages: qwd_cmessage { $$=$1; }
		| qwd_cmessages qwd_cmessage
			{ $$=node_add_next($1,$2); };

qwd_t12r: qwd_mcs_t12r {
			CO_t t12r_cs[] = {
			  { TOKEN_TIME,       1 },
			  { TOKEN_SEQ1,       1 },
			  { TOKEN_SEQ2,       1 },
			  { TOKEN_RELIABLE,   0 },
			};
			node* a = node_init(H_NOTHING,$1,0);
			reorder_commands(a,t12r_cs,arraylength(t12r_cs));
			$$=$1;
			a->down=NULL;
			node_delete(a);
		};

qwd_mcs_t12r: qwd_mc_t12r { $$=$1; }
		| qwd_mcs_t12r qwd_mc_t12r { $$=node_add_next($1,$2); }

qwd_mc_t12r: m_time
		| m_seq1
		| m_seq2
		| m_reliable
		;

dm2_messages: dm2_anycastmessage {
			$$=$1;
		}
		| dm2_messages dm2_anycastmessage {
			$$=node_add_next($1,$2);
		};

dm2_anycastmessage: m_unicast dm2_message {
			$$=node_add_next($1,$2);
		}
		| dm2_message {
			$$=$1;
		};

dem_message: m_unknown  { $$=$1; }
		| m_bad { $$=$1; }
		| m_nop { $$=$1; }
		| m_disconnect { $$=$1; }
		| dem_m_updatestat { $$=$1; }
		| m_version { $$=$1; }
		| m_setview { $$=$1; }
		| dem_m_sound { $$=$1; }
		| m_time { $$=$1; }
		| dem_m_print { $$=$1; }
		| m_stufftext { $$=$1; }
		| m_setangle { $$=$1; }
		| dem_m_serverinfo { $$=$1; }
		| m_lightstyle { $$=$1; }
		| m_updatename { $$=$1; }
		| m_updatefrags { $$=$1; }
		| m_clientdata { $$=$1; }
		| dem_m_stopsound { $$=$1; }
		| m_updatecolors { $$=$1; }
		| m_particle { $$=$1; }
		| m_damage { $$=$1; }
		| dem_m_spawnstatic { $$=$1; }
		| m_spawnbinary { $$=$1; }
		| dem_m_spawnbaseline { $$=$1; }
		| dem_m_temp_entity { $$=$1; }
		| m_setpause { $$=$1; }
		| m_signonnum { $$=$1; }
		| m_centerprint { $$=$1; }
		| m_killedmonster { $$=$1; }
		| m_foundsecret { $$=$1; }
		| m_spawnstaticsound { $$=$1; }
		| dem_m_intermission { $$=$1; }
		| m_finale { $$=$1; }
		| dem_m_cdtrack { $$=$1; }
		| m_sellscreen { $$=$1; }
		| m_cutscene { $$=$1; }
		| m_updateentity { $$=$1; };

qwd_sgamemessage: m_unknown
		| m_bad
		| m_disconnect
		| qwd_m_updatestat
		| qwd_m_sound
		| qwd_dm2_m_print
		| m_stufftext
		| m_setangle
		| qwd_m_serverdata
		| m_lightstyle
		| m_updatefrags
		| qwd_m_stopsound
		| m_damage
		| qwd_m_spawnstatic
		| qwd_m_spawnbaseline
		| qwd_m_temp_entity
		| m_setpause
		| m_centerprint
		| m_killedmonster
		| m_foundsecret
		| m_spawnstaticsound
		| qwd_m_intermission
		| m_finale
		| qwd_m_cdtrack
		| m_sellscreen
		| m_smallkick
		| m_bigkick
		| m_updateping
		| m_updateentertime
		| m_updatestatlong
		| qwd_m_muzzleflash
		| m_updateuserinfo
		| m_download
		| qwd_m_playerinfo
		| m_nails
		| m_choke
		| m_modellist
		| m_soundlist
		| qwd_m_packetentities
		| qwd_m_deltapacketentities
		| m_maxspeed
		| m_entgravity
		| m_setinfo
		| qwd_m_serverinfo
		| m_updatepl
		;

qwd_sconnlessmessage: m_unknown
		| connless_m_disconnect
		| m_client_command
		| m_challenge
		| m_connect
		| m_ping
		| m_console
		;

qwd_cmessage: m_time
		| c_load { $$=$1; $$->down->hint=H_LONG; }
		| m_angles
		| m_speed
		| c_fire
		| c_jump
		| c_flag
		| c_impulse
		| m_uk_angles
		;

dm2_message: m_unknown  { $$=$1; }
		| m_bad { $$=$1; }
		| dm2_m_muzzleflash { $$=$1; }
		| m_muzzlflash2 { $$=$1; }
		| dm2_m_temp_entity { $$=$1; }
		| m_layout { $$=$1; }
		| m_inventory { $$=$1; }
		| m_nop { $$=$1; }
		| m_disconnect { $$=$1; }
		| m_reconnect { $$=$1; }
		| dm2_m_sound { $$=$1; }
		| qwd_dm2_m_print { $$=$1; }
		| m_stufftext { $$=$1; }
		| dm2_m_serverdata { $$=$1; }
		| m_configstring { $$=$1; }
		| dm2_m_spawnbaseline { $$=$1; }
		| m_centerprint { $$=$1; }
		| m_download { $$=$1; }
		| dm2_m_playerinfo { $$=$1; }
		| dm2_m_packetentities { $$=$1; }
		| dm2_m_deltapacketentities { $$=$1; }
		| m_frame { $$=$1; }


m_camera: TOKEN_CAMERA floatvector ';' {
			$$=node_add_down($1,$2);
		};

m_unknown: TOKEN_UNKNOWN '{' mcs_unknown '}' {
			$$=node_add_down($1,$3);
			$$->hint = H_UNKNOWN;
		};

mcs_unknown: c_id c_data { $$=node_add_next($1,$2); }
		| c_data c_id { $$=node_add_next($2,$1); };

m_bad: TOKEN_BAD ';' {
			$$=$1;
			$$->hint = H_SIMPLE;
		};

m_nop: TOKEN_NOP ';' {
			$$=$1;
			$$->hint = H_SIMPLE;
		};

m_disconnect: TOKEN_DISCONNECT ';' {
			$$=$1;
			$$->hint = H_SIMPLE;
		};

connless_m_disconnect: TOKEN_DISCONNECT v_string ';' {
			$$=node_add_down($1,$2);
			$$->hint = H_SIMPLE;
		};

m_reconnect: TOKEN_RECONNECT ';' {
			$$=$1;
			$$->hint = H_SIMPLE;
		};

dem_m_updatestat: TOKEN_UPDATESTAT '{' mcs_updatestat '}' {
			$$=node_add_down($1,$3);
			$$->hint = H_SIMPLE;
		};

qwd_m_updatestat: TOKEN_UPDATESTAT '{' mcs_updatestat '}' {
			$3->next->down->hint=H_BYTE;
			$$=node_add_down($1,$3);
			$$->hint = H_SIMPLE;
		};

mcs_updatestat: c_index c_value { $$=node_add_next($1,$2); }
		| c_value c_index { $$=node_add_next($2,$1); };

m_version: TOKEN_VERSION v_int ';' {
			$2->hint = H_LONG;
			$$=node_add_down($1,$2);
			$$->hint = H_SIMPLE;
		};

m_setview: TOKEN_SETVIEW v_int ';' {
			$2->hint = H_SHORT;
			$$=node_add_down($1,$2);
			$$->hint = H_SIMPLE; 
		};

dem_m_sound: TOKEN_SOUND '{' dem_qwd_mcs_sound '}' {
			CO_t sound_cs[] = {
			  { TOKEN_VOL,         0 },
			  { TOKEN_ATTENUATION, 0 },
			  { TOKEN_CHANNEL,     1 },
			  { TOKEN_ENTITY,      1 },
			  { TOKEN_SOUNDNUM,    1 },
			  { TOKEN_ORIGIN,      1 }
			};
			$$=node_add_down($1,$3);
			$$->hint = H_DEM_SOUND;
			reorder_commands($$,sound_cs,arraylength(sound_cs));
        };

qwd_m_sound: TOKEN_SOUND '{' dem_qwd_mcs_sound '}' {
			CO_t sound_cs[] = {
			  { TOKEN_CHANNEL,     1 },
			  { TOKEN_ENTITY,      1 },
			  { TOKEN_VOL,         0 },
			  { TOKEN_ATTENUATION, 0 },
			  { TOKEN_SOUNDNUM,    1 },
			  { TOKEN_ORIGIN,      1 }
			};
            		$$=node_add_down($1,$3);
			$$->hint = H_QWD_SOUND;
			reorder_commands($$,sound_cs,arraylength(sound_cs));
        };

dem_qwd_mcs_sound: dem_qwd_mc_sound {
			$$=$1;
		}
		| dem_qwd_mcs_sound dem_qwd_mc_sound {
			$$=node_add_next($1,$2);
		};

dem_qwd_mc_sound: c_vol { $$=$1; }
		| c_attenuation { $$=$1; }
		| c_channel { $$=$1; }
		| c_entity { $$=$1; }
		| c_soundnum { $$=$1; }
		| c_origin { $$=$1; };

dm2_m_sound: TOKEN_SOUND '{' dm2_mcs_sound '}' {
			CO_t sound_cs[] = {
			  { TOKEN_UNKNOWN_MASK_BITS, 0 },
			  { TOKEN_SOUNDNUM,    1 },
			  { TOKEN_VOL,         0 },
			  { TOKEN_ATTENUATION, 0 },
			  { TOKEN_TIMEOFS,     0 },
			  { TOKEN_CHANNEL,     0 },
			  { TOKEN_ENTITY,      0 },
			  { TOKEN_ORIGIN,      0 }
			};
			$$=node_add_down($1,$3);
			$$->hint = H_DM2_SOUND;
			reorder_commands($$,sound_cs,arraylength(sound_cs));
        };

dm2_mcs_sound: dm2_mc_sound { $$=$1; }
		| dm2_mcs_sound dm2_mc_sound { $$=node_add_next($1,$2); };

dm2_mc_sound: c_unknown_mask_bits
		| c_soundnum
		| c_vol
		| c_attenuation
		| c_timeofs
		| c_channel
		| c_entity
		| c_origin;

m_time: TOKEN_TIME time ';' {
			$$=node_add_down($1,$2);
			$$->hint = H_SIMPLE;
		};

dem_m_print: TOKEN_PRINT v_string ';' {
			$2->hint = H_STRING;
			$$=node_add_down($1,$2);
			$$->hint = H_SIMPLE;
		};

qwd_dm2_m_print: TOKEN_PRINT '{' qwd_dm2_mcs_print '}' {
			$$=node_add_down($1,$3);
			$$->hint = H_SIMPLE;
		};

qwd_dm2_mcs_print: c_level c_string { $$=node_add_next($1,$2); }
		| c_string c_level { $$=node_add_next($2,$1); };

m_console: TOKEN_CONSOLE v_string ';' {
			$2->hint = H_STRING;
			$$=node_add_down($1,$2);
			$$->hint = H_SIMPLE;
		};

m_stufftext: TOKEN_STUFFTEXT v_string ';' {
			$2->hint = H_STRING;
			$$=node_add_down($1,$2);
			$$->hint = H_SIMPLE;
		};

m_setangle: TOKEN_SETANGLE floatvector ';' {
			node_add_hint_next($2,H_ANGLE);
			$$=node_add_down($1,$2);
			$$->hint = H_SIMPLE;
		};

dem_m_serverinfo: TOKEN_SERVERINFO '{' dem_mcs_serverinfo '}' {
			CO_t serverinfo_cs[] = {
			  { TOKEN_SERVERVERSION, 1 },
			  { TOKEN_MAXCLIENTS,    1 },
			  { TOKEN_MULTI,         1 },
			  { TOKEN_MAPNAME,       1 },
			  { TOKEN_MODELS,        1 },
			  { TOKEN_SOUNDS,        1 }
			};
			$$=node_add_down($1,$3);
			$$->hint = H_SIMPLE;
			reorder_commands($$, serverinfo_cs, arraylength(serverinfo_cs));
		};

dem_mcs_serverinfo: dem_mc_serverinfo {	$$=$1;	}
		| dem_mcs_serverinfo dem_mc_serverinfo {
			$$=node_add_next($1,$2);
		};

dem_mc_serverinfo: c_serverversion { $$=$1; }
		| c_maxclients { $$=$1; }
		| c_multi { $$=$1; }
		| c_mapname { $$=$1; }
		| c_models { $$=$1; }
		| c_sounds { $$=$1; };

qwd_m_serverdata: TOKEN_SERVERDATA '{' qwd_mcs_serverdata '}' {
			CO_t serverdata_cs[] = {
			  { TOKEN_SERVERVERSION, 1 },
			  { TOKEN_AGE,           1 },
			  { TOKEN_GAME,          1 },
			  { TOKEN_CLIENT,        1 },
			  { TOKEN_MAPNAME,       1 },
			  { TOKEN_UK_FLOATS,     1 }
			};
			$$=node_add_down($1,$3);
			$$->hint = H_SIMPLE;
			reorder_commands($$, serverdata_cs, arraylength(serverdata_cs));
		};

qwd_mcs_serverdata: qwd_mc_serverdata {
			$$=$1;
		}
		| qwd_mcs_serverdata qwd_mc_serverdata {
			$$=node_add_next($1,$2);
		};

qwd_mc_serverdata: c_serverversion { $$=$1; }
		| c_age { $$=$1; }
		| c_game { $$=$1; }
		| c_client { $1->down->hint=H_BYTE; $$=$1; }
		| c_mapname { $$=$1; };
		| c_uk_floats { $$=$1; };

dm2_m_serverdata: TOKEN_SERVERDATA '{' dm2_mcs_serverdata '}' {
			CO_t serverdata_cs[] = {
			  { TOKEN_SERVERVERSION, 1 },
			  { TOKEN_KEY,           1 },
			  { TOKEN_ISDEMO,        1 },
			  { TOKEN_GAME,          1 },
			  { TOKEN_CLIENT,        1 },
			  { TOKEN_MAPNAME,       1 }
			};
			$$=node_add_down($1,$3);
			$$->hint = H_SIMPLE;
			reorder_commands($$, serverdata_cs, arraylength(serverdata_cs));
		};

dm2_mcs_serverdata: dm2_mc_serverdata {
			$$=$1;
		}
		| dm2_mcs_serverdata dm2_mc_serverdata {
			$$=node_add_next($1,$2);
		};

dm2_mc_serverdata: c_serverversion { $$=$1; }
		| c_key { $$=$1; }
		| c_isdemo { $$=$1; }
		| c_game { $$=$1; }
		| c_client { $$=$1; }
		| c_mapname { $$=$1; };

m_lightstyle: TOKEN_LIGHTSTYLE '{' mcs_lightstyle '}' {
			$$=node_add_down($1,$3);
			$$->hint = H_SIMPLE;
		};

mcs_lightstyle: c_style c_string {
			$$=node_add_next($1,$2);
		}
		| c_string c_style {
			$$=node_add_next($2,$1);
		};

m_configstring: TOKEN_CONFIGSTRING '{' mcs_configstring '}' {
			$$=node_add_down($1,$3);
			$$->hint = H_SIMPLE;
		};

mcs_configstring: c_index c_string {
			$1->down->hint = H_SHORT;
			$$=node_add_next($1,$2);
		}
		| c_string c_index {
			$2->down->hint = H_SHORT;
			$$=node_add_next($2,$1);
		};

m_updatename: TOKEN_UPDATENAME '{' mcs_updatename '}' {
			$$=node_add_down($1,$3);
			$$->hint = H_SIMPLE;
		};

mcs_updatename: c_player c_netname { $$=node_add_next($1,$2); }
		| c_netname c_player { $$=node_add_next($2,$1);	};

m_updatefrags: TOKEN_UPDATEFRAGS '{' mcs_updatefrags '}' {
			$$=node_add_down($1,$3);
			$$->hint = H_SIMPLE;
		};

mcs_updatefrags: c_player c_frags { $$=node_add_next($1,$2); }
		| c_frags c_player { $$=node_add_next($2,$1); };

m_clientdata: TOKEN_CLIENTDATA '{' mcs_clientdata '}' {
			CO_t clientdata_cs[] = {
			  { TOKEN_VIEW_OFS_Z,   0 },
			  { TOKEN_PUNCHANGLE_X, 0 },
			  { TOKEN_ANGLES_1,     0 },
			  { TOKEN_ANGLES_2,     0 },
			  { TOKEN_ANGLES_3,     0 },
			  { TOKEN_VEL_X,        0 },
			  { TOKEN_VEL_Y,        0 },
			  { TOKEN_VEL_Z,        0 },
			  { TOKEN_WEAPONS,      0 },
			  { TOKEN_AMMOTYPE,     0 },
			  { TOKEN_ARMORTYPE,    0 },
			  { TOKEN_MEGAHEALTH,   0 },
			  { TOKEN_KEYS,         0 },
			  { TOKEN_POWER_UPS,    0 },
			  { TOKEN_UK_ITEMS_B23, 0 },
			  { TOKEN_UK_ITEMS_B24, 0 },
			  { TOKEN_UK_ITEMS_B25, 0 },
			  { TOKEN_UK_ITEMS_B26, 0 },
			  { TOKEN_UK_ITEMS_B27, 0 },
			  { TOKEN_RUNES,        0 },
			  { TOKEN_UK_BIT_B10,   0 },
			  { TOKEN_UK_BIT_B11,   0 },
			  { TOKEN_WEAPONFRAME,  0 },
			  { TOKEN_ARMORVALUE,   0 },
			  { TOKEN_WEAPONMODEL,  0 },
			  { TOKEN_HEALTH,       1 },
			  { TOKEN_CURRENTAMMO,  1 },
			  { TOKEN_AMMO_SHELLS,  1 },
